						$nationality = $_POST['nationality']; 
						$category = $_POST['category']; 
						
					    $bool = true;
						
					    if ($bool) {
				        mysqli_query($connection, "insert into studentpersonaldetail(name,fathername,mothername,address,gender,date_of_birth,email,phone_number,collage_name,register_number,branch,nationality,category) 
						values ('$name ','$fathername','$mothername','$address','$gender','$date_of_birth','$email','$phone_number','$collage_name','$register_number','$branch','$nationality','$category',)");
						Print '<script>alert("Successfully Send Status !");</script>';
                        Print '<script>window.location.assign("studentpersonaldetils.php");</script>';
						}
                        }
                        ?>













						
