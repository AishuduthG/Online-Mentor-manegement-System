mentor check login
 <?php

	    include("config.php");
	    session_start();
	    $email = mysqli_real_escape_string($connection, $_POST['email']);
	    $password = mysqli_real_escape_string($connection, $_POST['password']);
 
        
        $query = mysqli_query($connection, "SELECT * from addmentor WHERE  password='$password' and email='$email'");
	    $exists = mysqli_num_rows($query);
	    $table_reg = "";
	    $table_password = "";
		$mid;
		$mentor_name;
		$address;
		$dob;
		$mobile;
		$password;
	    if($exists > 0) 
	    {
	    while($row = mysqli_fetch_assoc($query)) 
		{
		$table_reg = $row['email'];
		$table_password = $row['password'];
		$mid = $row['mid'];
		$mentor_name = $row['mentor_name'];
		$address = $row['address'];
		$dob = $row['dob'];
		$mobile = $row['mobile'];
		$password = $row['password'];
		}
		if(($email == $table_reg) && ($password == $table_password))
		{
		if($password == $table_password)
		{
			
			$_SESSION['user'] = $email;
			$_SESSION['mid'] = $rid;
			$_SESSION['mentor_name'] = $mentor_name;
			$_SESSION['address'] = $address;
			$_SESSION['dob'] = $dob;
			$_SESSION['mobile'] = $mobile;
			$_SESSION['password'] = $password;
			
		
		header("location: mentordashboard.php");
		}
     	}
		else
    	{
	    Print '<script>alert("Successfully Login");</script>'; 
		Print '<script>window.location.assign("mentordashboard.php");</script>'; 
		}
	    }
	    else{
		Print '<script>alert("Incorrect password!");</script>'; 
		Print '<script>window.location.assign("mentorlogin.php");</script>';
	    }  
        ?>
