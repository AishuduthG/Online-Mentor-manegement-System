   					    <?php
                        include('config.php');
						if (isset($_POST['submit'])) 
						{
					
						$stname = $_POST['stname']; 
						$stdepartment_name = $_POST['stdepartment_name']; 
						$streg = $_POST['streg']; 
						$stdob = $_POST['stdob']; 
						$stemail = $_POST['stemail']; 
					    $stmobile = $_POST['stmobile'];
					    $parentname = $_POST['parentname'];
                        $address = $_POST['address'];
                        $parentmob = $_POST['parentmob'];
                        $genderstd = $_POST['genderstd'];
                       
    $bool = true;
	$querychk = mysqli_query($connection, "Select * from studentinfo");
    while ($row = mysqli_fetch_array($querychk)) {
    $table_reg = $row['stemail'];
    if ($stemail == $table_reg) {
    $bool = false;
    Print '<script>alert("'. $row['stemail'] .' has already Update Info ...");</script> ';
    Print '<script>window.location.assign("ADDinfoStudent.php");</script>';
    }
	}
						if ($bool) {
				        mysqli_query($connection, "insert into studentinfo(stname,stdepartment_name,streg,stdob,stemail,stmobile,parentname,address,parentmob,genderstd) 
						values ('$stname ','$stdepartment_name','$streg','$stdob','$stemail','$stmobile','$parentname','$address','$parentmob','$genderstd')");
					    Print '<script>alert("Successfully Updated Info!");</script>';
                        Print '<script>window.location.assign("ADDinfoStudent.php");</script>';
				        }
                        }
                        ?>
	
