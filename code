		                <?php
                        include('config.php');
					    if (isset($_POST['submit'])) {
                       
                        $mentor_name = $_POST['mentor_name'];
						$student_name = $_POST['student_name'];
						$mark1 = $_POST['mark1'];
						$mark2 = $_POST['mark2'];
						$mark3 = $_POST['mark3'];
						$mark4 = $_POST['mark4'];
						$mark5 = $_POST['mark5'];
						$mark6 = $_POST['mark6'];
						$ia_num = $_POST['ia_num'];
						
						
						
                        $bool = true;
                  
                        if ($bool) {
                        mysqli_query($connection, "insert into addmark(mentor_name,student_name,mark1,mark2,mark3,mark4,mark5,mark6,ia_num)
						values ('$mentor_name','$student_name','$mark1','$mark2','$mark3','$mark4','$mark5','$mark6','$ia_num')");
