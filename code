		                <?php
                        include('config.php');
					    if (isset($_POST['submit'])) {
                       
                        $name = $_POST['name'];
						$department_name = $_POST['department_name'];
						$subject = $_POST['subject'];
						$studentassigned = $_POST['studentassigned'];
						$mobile_num = $_POST['mobile_num'];
						$email = $_POST['email'];
						$password = $_POST['password'];
                        $bool = true;
                  
                        if ($bool) {
                        mysqli_query($connection, "insert into addmentor(name,department_name,subject,studentassigned,mobile_num,email,password)
						values ('$name','$department_name','$subject','$studentassigned','$mobile_num','$email','$password')");
						if(mail($email,"Login Credentials","Dear $name, Use this Email '$email' and Password ' $password ' to login your Account")){
						echo '<script>alert("Your mail has been sent successfully");</script>';
						}else{
						echo '<script>alert("Unable to send email. Please try again");</script>';
					    }
                        Print '<script>window.location.assign("mentorregestration.php");</script>';
                        }
			            }
						
                        ?>

						
						
						
